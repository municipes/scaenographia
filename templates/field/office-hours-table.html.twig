{% set random = random() %}
<div class="office-hours">
  <a class=""
    data-bs-toggle="collapse" href="#multiCollapseHours-{{ random }}"
    role="button" aria-expanded="false"
    aria-controls="multiCollapseHours-{{ random }}">
    {% include '@bi-bcl/icon/icon.html.twig' with {
      name: 'it-clock',
      size: 'sm',
      color: 'primary',
    } %}
    {{ 'Opening time'|t }}</a>
  <div class="collapse multi-collapse" id="multiCollapseHours-{{ random }}">{{ table }}</div>
</div>
