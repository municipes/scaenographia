<div class="point-list-wrapper my-4" data-element="service-calendar-list">
  <div class="point-list">
    <div class="point-list-aside point-list-primary">
      <div class="point-date font-monospace">{{ data_fase|render.__toString()|date('U')|format_date('custom','d') }}</div>
      <div class="point-month font-monospace">{{ data_fase|render.__toString()|date('U')|format_date('custom','M') }}</div>
    </div>
    <div class="point-list-content">
      <div class="card card-teaser shadow rounded">
        <div class="card-body">
          <h5 class="card-title">
            {{ data_fase|render.__toString()|date('U')|format_date('custom','H:i') }} - {{ testo|render.__toString()|striptags('<strong>')|raw }}
          </h5>
        </div>
      </div>
    </div>
  </div>
</div>
